{
  "name": "appschain",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "start": "npm run build-client & node index.js",
    "dev": "start npm run dev-redis && start npm run dev-client && cross-env ENV='development' && nodemon index.js",
    "dev-peer": "cross-env GENERATE_PEER_PORT='true' ENV='production' nodemon index.js",
    "build-client": "npm run clean && parcel build client/src/index.html --out-dir client/dist",
    "dev-client": "npm run dev-clean && parcel client/src/index.html --out-dir client/dist",
    "dev-clean": "rmdir /Q /S .cache && rmdir /Q /S client\\dist",
    "clean": "rm -rf .cache client/dist",
    "dev-redis": "cd ./redis && redis-server",
    "start-redis": "redis-server --daemonize yes"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.19.0",
    "elliptic": "^6.5.4",
    "express": "^4.17.1",
    "hex-to-binary": "^1.0.1",
    "history": "^4.10.1",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "redis": "^3.1.1",
    "request": "^2.88.0",
    "uuid": "^3.3.3"
  },
  "engines": {
    "node": "14.15.5"
  }
}
